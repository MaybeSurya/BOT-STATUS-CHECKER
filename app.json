{
    "name": "Bot_Status_Checker",
    "logo": "https://telegra.ph/file/ec9bdca88786b7f506038.jpg",
    "description": "Easy & powerful bot to check if your all Telegram bots are working or not. This bot status bot updates every 45 minutes & runs for 24x7 hours.",
    "repository": "https://github.com/SuryaModsYT/BOT-STATUS-CHECKER",
    "keywords": [
        "BotStatus",
        "How to check your bots are alive or not",
        "Up or down",
        "Check status of bots",
        "Telegram userbot",
        "Pyrogram userbot"
    ],
    "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "SESSION_STRING": {
            "description": "A valid Pyrogram session name",
            "required": true
        },
        "BOT_LIST": {
            "description": "Your bot username list without '@' (Example: surya_musicbot suryaro_bot suryaxsessionbot)",
            "required": true
        },
       "CHANNEL_OR_GROUP_ID": {
            "description": "Your channel's or group's Telegram id (Example: ---1001725447152)",
            "required": true
            "value": "-1001725447152"
        },
       "MESSAGE_ID": {
            "description": "Telegram id of message from your channel or group (Example: 10) Need Help Then Contact @SuryaModOwner",
            "required": true
        },
        "BOT_ADMIN_IDS": {
            "description": "Bot admin(s) id(s) (Example: 5043850742)"
            "value": "5043850742"
        },
        "TIME_ZONE": {
            "description": "Your time zone (Example: Asia/Kolkata)",
            "required": true
            "value": "Asia/Kolkata"
        }
    },
    "formation": {
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
